<%= request.query.inspect %>
<%
order = []
request.query.each do |key, value|
	if @order.index(key) != nil
		order[value.to_i] = key
	end
end
%>
<%= @order.inspect %>
<%= order.inspect %>
<%
@order = order.clone
save_db @db, @schema, @order, @file
response.redirect("/?m=Properties%20Rearranged")
%>
